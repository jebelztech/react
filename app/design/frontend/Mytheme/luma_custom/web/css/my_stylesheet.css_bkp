.header-login,
.header-profile,
.language-switcher {
    font-size: 15px;
    display: block;
    float: right;
    padding: 7px 10px 5px 25px;
    position: relative;
    color: #000;
}

.language-switcher {
    padding: 7px 25px 5px 25px;
}

.header-login::before,
.header-profile::before {
    content: '';
    background: url("../images/login.png");
    width: 35px;
    height: 25px;
    background-repeat: no-repeat;
    font-size: 41px;
    position: absolute;
    left: 3px;
    top: 9px;
    pointer-events: none;
    background-size: 16px;
}

.minicart-wrapper .action.showcart:before {
    background: url('../images/shopping-bag.png');
    content: "" !important;
    width: 25px;
    height: 25px;
    background-repeat: no-repeat;
    font-size: 41px;
    position: absolute;
    left: 3px;
    top: 9px;
    pointer-events: none;
    background-size: 16px;
}

.header-login a,
.header-profile a,
.language-switcher a {
    font-size: 14px;
    color: #000 !important;
    font-weight: 400;
}

.page-header .text {
    display: block;
    overflow: auto !important;
    width: auto !important;
    height: auto !important;
    float: right;
    font-size: 14px;
    color: #000;
    padding: 5px 0 5px 25px !important;
    position: relative !important;
}

#switcher-option {
    position: absolute;
    width: initial;
    font-size: 14px;
