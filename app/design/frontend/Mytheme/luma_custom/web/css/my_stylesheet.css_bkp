@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap');

body {
    box-sizing: border-box;
    padding: 0;
    margin: 0;
    counter-reset: counteing;
    position: relative;
    overflow-x: hidden;
    font-family: 'Poppins', sans-serif;
    background: #fefefe;
}

@font-face {
    font-family: barcode;
    src: url(../fonts/B39MHR.TTF);
}

@font-face {
    font-family: barcodebig;
    src: url(../fonts/Code39.ttf);
}

.section-items.nav-sections-items .nav-sections-item-content:nth-child(4),
