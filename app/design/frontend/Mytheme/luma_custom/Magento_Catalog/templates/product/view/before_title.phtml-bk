<script>
    require(['jquery'], function($, $t) {
                // Set the date we're counting down to
                $(document).ready(function() {
                    $("#click-me").click(function(){
                        $("#Delivery-Details").css({
                            'overflow': 'visible',
                            'visibility' : 'visible',
                        })
                        $("#Delivery-Details .modal-container").css({
                            'transform' : 'translateY(0%)',
                            '-webkit-transform' : 'translateY(0%)'
                        });
                    })
                    $(".delivery-days-shipto").click(function(){
                        $(".full-width-modal").css({
                            'overflow': 'visible',
                            'visibility' : 'visible',
                        })
                        $(".full-width-modal .modal-container").css({
                            'transform' : 'translateY(0%)',
                            '-webkit-transform' : 'translateY(0%)'
                        });
                        $("body").css({
                            'overflow' : 'hidden',
                        });
                    })

                    $(".btn-close").click(function(){
                        $(".modal-wrapper").css({
                            'overflow': 'hidden',
                            'visibility' : 'hidden',
                        })
                        $(".modal-wrapper .modal-container").css({
                            'transform' : 'translateY(-200%)',
                            '-webkit-transform' : 'translateY(-200%)'
                        });
                        $("body").css({
                            'overflow' : 'visible',
                        });
                    });

                    $(".continent-name").click(function(){
                        $(".countries").removeClass('active');
                        $(this).closest(".countries").addClass('active');
                    })
                });
                });
</script>